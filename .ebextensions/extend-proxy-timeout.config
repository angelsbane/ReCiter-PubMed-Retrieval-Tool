files:
  "/etc/nginx/conf.d/extend-proxy-timeout.conf":
    mode: "644"
    owner: "root"
    group: "root"
    content: |
    
      proxy_read_timeout          300;
      proxy_connect_timeout       300;
      proxy_send_timeout          300;
      send_timeout                300;

container_commands:
  01_restart_nginx:
    command: "sudo service nginx reload"
